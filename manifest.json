{
    "manifest_version": 3,
    "name": "NoMeet",
    "description": "哇哈哈哈哈哈哈",
    "version": "0.1",
    "background": {
        "service_worker": "background.js"
     },
     "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/img16.png",
            "32": "images/img32.png",
            "48": "images/img48.png",
            "128": "images/img128.png"
        }
     },
     "web_accessible_resources": [
        {
           "resources": [
            "images\t.png"
           ],
           "matches": [
              "*://*/*"
           ]
        }
     ],
    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "storage"
    ],
    "icons": {
        "16": "images/img16.png",
        "32": "images/img32.png",
        "48": "images/img48.png",
        "128": "images/img128.png"
    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "css": [
                "content.css"
             ],
            "matches": [
               "https://meet.google.com/*"
            ]
        }
    ]
}